{
  "name": "Litecom2MQTT",
  "version": "0.1.0",
  "slug": "litecom2mqtt",
  "description": "Bridge Zumtobel Litecom CCD to MQTT with Home Assistant discovery.",
  "url": "https://github.com/swissmanu/litecom2mqtt",
  "startup": "services",
  "boot": "auto",
  "arch": [
    "aarch64",
    "amd64",
    "armv7",
    "armhf",
    "i386"
  ],
  "image": "ghcr.io/swissmanu/litecom2mqtt/litecom2mqtt:latest",
  "host_network": true,
  "ingress": false,
  "panel_icon": "mdi:lightbulb-group",
  "environment": {
    "LITECOM2MQTT_MQTT_BROKER_HOST": "CHANGE_ME_MQTT_BROKER_IP",
    "LITECOM2MQTT_MQTT_BROKER_PORT": "1883",
    "LITECOM2MQTT_MQTT_BROKER_PROTOCOL": "mqtt",
    "LITECOM2MQTT_MQTT_TOPIC_PREFIX": "litecom2mqtt",
    "LITECOM2MQTT_LOG_LEVEL": "INFO",
    "LITECOM2MQTT_LITECOM_HOST": "CHANGE_ME_LITECOM_IP",
    "LITECOM2MQTT_LITECOM_CONSUMER_NAME": "HomeAssistant",
    "LITECOM2MQTT_LITECOM_CONSUMER_API_KEY": "CHANGE_ME_API_KEY",
    "LITECOM2MQTT_LITECOM_STATE_MQTT_TOPIC_PREFIX": "litecom",
    "LITECOM2MQTT_LITECOM_QUERY_DEVICES": "true",
    "LITECOM2MQTT_LITECOM_QUERY_GROUPS": "true",
    "LITECOM2MQTT_LITECOM_QUERY_ROOMS": "false"
  },
  "options": {},
  "schema": {}
}