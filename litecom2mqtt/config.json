{
  "name": "Litecom2MQTT",
  "version": "0.1.9",
  "slug": "litecom2mqtt",
  "description": "Bridge Zumtobel Litecom CCD to MQTT with Home Assistant discovery.",
  "url": "https://github.com/swissmanu/litecom2mqtt",
  "startup": "services",
  "boot": "auto",
  "arch": [
    "aarch64",
    "amd64",
    "armv7",
    "armhf",
    "i386"
  ],
  "host_network": true,
  "ingress": false,
  "panel_icon": "mdi:lightbulb-group",
  "environment": {
    "LITECOM2MQTT_MQTT_BROKER_HOST": "192.168.1.250",
    "LITECOM2MQTT_MQTT_BROKER_PORT": "8883",
    "LITECOM2MQTT_MQTT_BROKER_PROTOCOL": "mqtt",
    "LITECOM2MQTT_MQTT_TOPIC_PREFIX": "litecom",
    "LITECOM2MQTT_LOG_LEVEL": "INFO",
    "LITECOM2MQTT_LITECOM_HOST": "192.168.1.254",
    "LITECOM2MQTT_LITECOM_CONSUMER_NAME": "hamqtt",
    "LITECOM2MQTT_LITECOM_CONSUMER_API_KEY": "k1tnqUYsADmXx6q467SRHOWVZetzWjoLvBNO7pXGjoI=",
    "LITECOM2MQTT_LITECOM_STATE_MQTT_TOPIC_PREFIX": "zones",
    "LITECOM2MQTT_LITECOM_QUERY_DEVICES": "true",
    "LITECOM2MQTT_LITECOM_QUERY_GROUPS": "true",
    "LITECOM2MQTT_LITECOM_QUERY_ROOMS": "false"
  }
} 
